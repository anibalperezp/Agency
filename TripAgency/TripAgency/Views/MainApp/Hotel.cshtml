    function caldoeffvr() {
        var start = $("#start").val();
        var end = $("#end").val();
        var endsp = start.split('/');
        var startsp = end.split('/');
        var datestart = new Date();
        var dateend = new Date();
        datestart.setTime(Date.parse(start));
        dateend.setTime(Date.parse(end));
        var dif = dateend - datestart;
        var iDay = ((dif / (60 * 60 * 24)) / 1000 - 1) + 1;
        date = iDay.toString() + " days (" + endsp[0].toLowerCase() + " " + endsp[1] + " - " + startsp[0].toLowerCase() + " " + startsp[1] + ")";
    }

